{
    "family": "flask-task",
    "containerDefinitions": [
        {
            "name": "flask-api",
            "image": "035578590291.dkr.ecr.us-east-1.amazonaws.com/flask-api:latest",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:5000 || exit 1"
                ],
                "interval": 60,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 30
            },
            "systemControls": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::035578590291:role/ecs-task-execution-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}
